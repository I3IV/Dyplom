{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 25,
    "max": 50
  },
  "mysql": "mysql://root:@localhost:3306/restaurantdb",
  "authentication": {
    "secret": "",
    "strategies": ["jwt", "local", "staff"],
    "path": "/authentication",
    "service": "customers",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "service": "customers",
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    },
    "staff": {
      "service": "staff",
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}
